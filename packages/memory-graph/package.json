{
  "name": "@aide/memory-graph",
  "version": "0.1.0",
  "type": "module",
  "description": "Memory graph engine for AIDE - persistent intent and state management",
  "main": "dist/core-only.js",
  "types": "dist/core-only.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core-only.d.ts",
      "import": "./dist/core-only.js",
      "default": "./dist/core-only.js"
    },
    "./core": {
      "types": "./dist/core-only.d.ts",
      "import": "./dist/core-only.js",
      "default": "./dist/core-only.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.core.json",
    "dev": "tsc -p tsconfig.core.json --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@reactflow/background": "^11.3.14",
    "@reactflow/controls": "^11.2.14",
    "@reactflow/core": "^11.11.4",
    "@reactflow/minimap": "^11.7.14",
    "clsx": "^2.1.1",
    "immutable": "^4.3.6",
    "lucide-react": "^0.379.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "rxjs": "^7.8.1",
    "rxjs-hooks": "0.8.0-alpha.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "jsdom": "^24.0.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
