{
  "name": "@aide/agent-runtime",
  "version": "0.1.0",
  "type": "module",
  "description": "Agent orchestration and execution runtime for AIDE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aide/memory-graph": "workspace:*",
    "@ai-sdk/openai": "^0.0.66",
    "@ai-sdk/anthropic": "^0.0.54",
    "@ai-sdk/google": "^0.0.52",
    "ai": "^4.0.12",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
