# Production Environment Variables for AIDE Landing Page
# Copy this to .env.local for production environment

# Application URLs
NEXT_PUBLIC_BASE_URL=https://aide.dev
NEXT_PUBLIC_SITE_URL=https://aide.dev
NEXT_PUBLIC_CONTROL_PANEL_URL=https://app.aide.dev

# Stripe Configuration (Production)
# Replace with your production Stripe keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_publishable_key_here
STRIPE_SECRET_KEY=sk_live_your_production_secret_key_here

# Stripe Price IDs (Production)
# Replace with your production price IDs from Stripe Dashboard
NEXT_PUBLIC_STRIPE_PROFESSIONAL_PRICE_ID=price_production_professional_monthly
NEXT_PUBLIC_STRIPE_PROFESSIONAL_YEARLY_PRICE_ID=price_production_professional_yearly
NEXT_PUBLIC_STRIPE_ENTERPRISE_PRICE_ID=price_production_enterprise_monthly
NEXT_PUBLIC_STRIPE_ENTERPRISE_YEARLY_PRICE_ID=price_production_enterprise_yearly
