# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Stripe Price IDs (get these from running the setup-stripe.js script)
NEXT_PUBLIC_STRIPE_PROFESSIONAL_PRICE_ID=price_professional_monthly
NEXT_PUBLIC_STRIPE_PROFESSIONAL_YEARLY_PRICE_ID=price_professional_yearly
NEXT_PUBLIC_STRIPE_ENTERPRISE_PRICE_ID=price_enterprise_monthly
NEXT_PUBLIC_STRIPE_ENTERPRISE_YEARLY_PRICE_ID=price_enterprise_yearly

# Control Panel URL - Update this to your deployed aide-control URL
NEXT_PUBLIC_CONTROL_PANEL_URL=https://your-aide-control-url.run.app

# Website URL for redirects
NEXT_PUBLIC_SITE_URL=http://localhost:3001

# Base URL for metadata
NEXT_PUBLIC_BASE_URL=https://aide.dev

# Optional: Analytics and tracking
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
