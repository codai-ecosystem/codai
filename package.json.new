{
  "name": "aide",
  "version": "0.1.0",
  "description": "AI-native development environment - conversational IDE for building software through natural language",
  "main": "dist/main.js",
  "homepage": "https://github.com/aide-dev/aide",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "build:packages": "turbo build --filter=@aide/*",
    "build:apps": "turbo build --filter=@aide/electron",
    "build:extensions": "turbo build --filter=aide-core",
    "dev": "turbo dev",
    "dev:packages": "turbo dev --filter=@aide/*",
    "dev:web": "vite serve packages/ui-components",
    "dev:api": "turbo dev --filter=@aide/agent-runtime",
    "test": "vitest",
    "test:packages": "turbo test --filter=@aide/*",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint \"packages/*/src/**/*.{ts,tsx}\" \"apps/*/src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"packages/*/src/**/*.{ts,tsx}\" \"apps/*/src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rimraf dist .turbo",
    "setup": "pnpm install && pnpm build",
    "prepare": "husky install",
    "start": "pnpm dev",
    "preview": "vite preview packages/ui-components",
    "aide:dev": "turbo dev --filter=@aide/electron",
    "aide:build": "turbo build --filter=@aide/electron",
    "aide:package": "turbo package --filter=@aide/electron",
    "agents:dev": "turbo dev --filter=@aide/agent-runtime",
    "memory:dev": "turbo dev --filter=@aide/memory-graph",
    "ui:dev": "turbo dev --filter=@aide/ui-components",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "@ai-sdk/anthropic": "^0.0.54",
    "@ai-sdk/google": "^0.0.52",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-button": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-router": "^1.78.4",
    "ai": "^4.0.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "framer-motion": "^11.11.17",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/express": "^5.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vitest": "^2.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aide-dev/aide.git"
  },
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.1.0",
  "workspaces": [
    "packages/*",
    "extensions/*",
    "apps/*"
  ],
  "keywords": [
    "ai",
    "ide",
    "development",
    "copilot",
    "agent",
    "conversation",
    "natural-language",
    "automation"
  ],
  "pnpm": {
    "overrides": {
      "minimist@<0.2.1": ">=0.2.1",
      "minimatch@<3.0.5": ">=3.0.5",
      "minimist@<0.2.4": ">=0.2.4",
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}
