LOGAI_API_URL=https://logai.codai.ro
MEMORAI_API_URL=https://memorai.codai.ro
CODAI_CENTRAL_API=https://api.codai.ro

# Authentication (LogAI Integration)
NEXTAUTH_URL=https://codai.ro
NEXTAUTH_SECRET=${NEXTAUTH_SECRET}
